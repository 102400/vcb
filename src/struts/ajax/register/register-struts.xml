<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="ajaxCheckUserEmail" extends="myJsonDefault" namespace="/ajax/register/check">
		<action name="useremail" class="struts.ajax.register.CheckUserEmailAction" method="execute">
			<result name="r" type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>
	<package name="ajaxCheckUserName" extends="myJsonDefault" namespace="/ajax/register/check">
		<action name="username" class="struts.ajax.register.CheckUserNameAction" method="execute">
			<result name="r" type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>
</struts>