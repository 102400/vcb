<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="ajaxOrderProcess" extends="myJsonDefault" namespace="/ajax/order">
		<action name="process" class="struts.ajax.order.process.ProcessAction" method="execute">
			<result name="r" type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>
	<package name="ajaxOrderCreate" extends="myJsonDefault" namespace="/ajax/order">
		<action name="create" class="struts.ajax.order.process.CreateAction" method="execute">
			<result name="r" type="json">
				<param name="root">result</param>
			</result>
		</action>
	</package>
</struts>